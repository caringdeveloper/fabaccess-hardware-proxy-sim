{
  "name": "fab-access-hardware-proxy-simulator",
  "version": "0.0.1",
  "description": "FabAccess Hardware Proxy - Bridge between backend and machines",
  "main": "build/main.js",
  "scripts": {
    "build": "npm run generate && npm run compile",
    "generate": "./node_modules/.bin/tsoa routes && tsoa swagger",
    "swagger": "./node_modules/.bin/tsoa swagger",
    "compile": "tsc -p .",
    "watch": "tsc -w",
    "main": "cross-env JWT_SECRET_KEY=test node ./build/main.js",
    "main:production": "cross-env JWT_SECRET_KEY=test NODE_ENV=production node ./build/main.js"
  },
