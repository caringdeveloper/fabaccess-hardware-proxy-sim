    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/method-override": "^0.0.31",
    "@types/node": "^12.12.5",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "nodemon": "^1.19.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@types/shortid": "^0.0.29",
    "axios": "^0.19.0",
